alias \
\
c="clear" \
v="nvim" \
l="ls -AFhl --color=auto --group-directories-first" \
ls="ls -Fh --color=auto --group-directories-first" \
ll="ls -aFhl --color=auto --group-directories-first" \
grep="grep --color=auto" \
mv="mv -i" \
mp="mkdir -p" \
rm="rm -Iv" \
df="df -h" \
du="du -h -d 1" \
k="killall" \
open="xdg-open"
